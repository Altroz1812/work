# Database
DATABASE_URL=postgresql://username:password@localhost:5432/workflow_db

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=24h

# Server
PORT=3000
NODE_ENV=development

# AI Services (Mock endpoints for Phase 1)
AI_SCORE_ENDPOINT=http://localhost:3000/api/mock/ai/score
